{
    "session_properties": {
        "output_format": "all",
        "session_name": "EMON-Trace",
        "buffer_size": 256,
        "minimum_buffers": 12,
        "maximum_buffers": 48,
        "flush_timer": 1,
        "event_count_max": 100
    },
    "user_providers": [
        {
            "name": "Microsoft-Windows-Kernel-Process"
        }
    ],
    "ZZZZkernel_providers": [
        {
            "name": "process"
        }
    ],
    "ZZuser_providers": [
        {
            "name": "Microsoft-Windows-Kernel-Process",
            "keywords_any": 16
        }
    ],
    "ZZkernel_providers": [
        {
            "name": "process",
            "filters": [
                {
                    "property_icontains": {
                        "name": "CommandLine",
                        "value": "notepad"
                    }
                }
            ]
        }
    ]
}
