{
    "session_properties": {
        "output_format": "stdout",
        "session_name": "EMon-Trace",
        "buffer_size": 256,
        "minimum_buffers": 12,
        "maximum_buffers": 48,
        "flush_timer": 1
    },
    "kernel_providers": [
        {
            "name": "process",
            "filters": {
                "all_of": {
                    "any_field_contains": "notepad"
                }
            }
        }

    ],
        "ZZZuser_providers":
        [
            {
                "name": "Microsoft-Windows-Kernel-Process",
                "keywords_any": 16,
                "filters": {
                    "ZZZany_of": {
                        "id_is": [ 1, 2 ],
                        "opcode_is": 1
                    },
                    "any_of": {
                        "ZZZid_is_max": [
                            {
                                "id_is": 1,
                                "max_events_total": 1
                            }
                        ],
                        "ZZZmax_events": 3,
                        "any_field_contains": "notepad.exe",
                        "ZZZZany_field_contains": [
                            {
                                "name": "ImageName",
                                "value": "notepad.exe",
                                "type": "STRINGA"
                            },
                            {
                                "name": "ImageName",
                                "value": "notepad.exe",
                                "type": "STRINGW"
                            }
                        ]
                    }
                }
            }
        ]
}
